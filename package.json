{
  "name": "@accelist/expo-starter",
  "version": "0.1.0",
  "description": "React Native Expo project starter template for PT. Accelist Lentera Indonesia",
  "author": "Ryan Elian",
  "homepage": "https://github.com/accelist/expo-starter#readme",
  "bugs": "https://github.com/accelist/expo-starter/issues",
  "license": "Apache-2.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typescript": "tsc -w --pretty --preserveWatchOutput",
    "eslint": "esw -w --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "pre-commit-check": "tsc && eslint --ext .js,.jsx,.ts,.tsx ./",
    "dev": "concurrently \"npm:prepare\" \"npm:typescript\" \"npm:eslint\""
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@tanstack/react-query": "^4.20.4",
    "expo": "^47.0.0",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.40.0",
    "react-native": "0.70.5",
    "react-native-paper": "^5.0.2",
    "react-native-web": "~0.18.7",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@react-native-community/eslint-config": "3.2.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "concurrently": "^7.6.0",
    "eslint": "8.29.0",
    "eslint-watch": "8.0.0",
    "husky": "^8.0.2",
    "typescript": "^4.6.3"
  }
}
